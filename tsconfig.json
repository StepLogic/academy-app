{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@res/*": ["./src/res/*"],
      "@utils/*": ["./src/utils/*"],
      "@contexts/*": ["./src/contexts/*"],
      "@stack/*": ["./src/stack/*"],
      "@assets/*": ["./assets/*"],
      "@api/*": ["./src/api/*"]
    }
  }
}
